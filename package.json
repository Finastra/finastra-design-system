{
  "name": "@finastra/finastra-design-system",
  "version": "3.5.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/finastra/finastra-design-system.git"
  },
  "license": "MIT",
  "workspaces": [
    "themes/angular-theme",
    "themes/fds-theme",
    "libs/web-components/*"
  ],
  "scripts": {
    "add:component": "nx workspace-schematic angular-component",
    "postadd:component": "nx format:write",
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "postaffected:test": "node ./scripts/jest-join-coverage.js",
    "build": "ng build",
    "build:devkit": "tsc -p ./tools/devkit/tsconfig.devkit.json && cpx -v \"./tools/devkit/**/*.json\" dist/tools/devkit",
    "build:fds-test-app": "ng build fds-test-app",
    "build:theme": "ng build angular-theme --prod",
    "build:fds-theme": "npm run build -w themes/fds-theme",
    "build:scripts": "tsc --project scripts/tsconfig.json",
    "changelog:components": "cd libs/angular-components && npx standard-version",
    "changelog:theme": "cd themes/angular-theme && npx standard-version",
    "clean": "rimraf dist coverage junit.xml",
    "dep-graph": "nx dep-graph",
    "dev": "npm start -- -o",
    "e2e": "ng e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "postinstall": "node ./scripts/skip.js || ngcc --properties es2020 browser module main && node ./decorate-angular-cli.js",
    "lint": "nx run-many --target=lint --all --exclude=angular-components",
    "ng": "nx",
    "nx": "nx",
    "release:angular": "npx standard-version -an",
    "start": "ng serve",
    "start:fds-test-app": "ng serve fds-test-app",
    "pretest": "rimraf ./coverage ./testresults",
    "test": "ng test",
    "posttest": "node ./scripts/jest-join-coverage.js",
    "testjest": "jest",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "wc:build": "npm run wc:build:style && npm run wc:build:ts -- --verbose",
    "wc:build:style": "npm run build:style -w libs/web-components",
    "wc:build:ts": "tsc --build libs/web-components",
    "wc:build:storybook": "npm run build:fds-theme && npm run wc:build && build-storybook -o storybook-static",
    "wc:build:bundle":"npm run build -w libs/web-components",
    "wc:compliance:check": "node ./scripts/compliance-check.js",
    "wc:format": "",
    "wc:generate": "nx workspace-generator web-component",
    "wc:serve": "npm run build:fds-theme && concurrently npm:wc:watch wds --open",
    "wc:storybook": "start-storybook -p 8080",
    "wc:test": "web-test-runner --coverage",
    "wc:test:watch": "npm run wc:test -- --watch",
    "wc:watch": "npm run wc:build --watch && node scripts/watcher.js",
    "wc:clean": "rimraf libs/web-components/**/dist libs/web-components/**/*.css.ts libs/web-components/**/node_modules",
    "wc:bump-all-wc-deps": "npm run build:scripts && node scripts/tsc-out/bump-all-wc-deps.js",
    "workspace-generator": "nx workspace-generator",
    "wca": "wca analyze 'libs/web-components/*/src/*.ts' --outFiles {dir}/../stories/sb-generated/custom-element.json",
    "postwca": "node ./scripts/wca-to-storybook.js",
    "prewc:build:storybook": "npm run wca",
    "prewc:storybook": "npm run wca",
    "add": "npm install --package-lock-only --no-package-lock --prefix",
    "postadd": "npm install",
    "chromatic": "npx chromatic --build-script-name=wc:build:storybook --exit-zero-on-changes"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.*": [
      "nx format:write --uncommited"
    ],
    "{apps,libs,tools,themes}/**/*.{ts,json,scss,html}": [
      "nx affected:lint --uncommitted --parallel --fix=true"
    ]
  },
