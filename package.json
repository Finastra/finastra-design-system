{
  "name": "@finastra/finastra-design-system",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/finastra/finastra-design-system.git"
  },
  "scripts": {
    "ng": "nx",
    "clean": "rimraf dist coverage junit.xml",
    "start": "ng serve",
    "build": "ng build",
    "build:theme": "ng build angular-theme --prod",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points && node ./decorate-angular-cli.js",
    "build:devkit": "tsc -p ./tools/devkit/tsconfig.devkit.json && cpx -v \"./tools/devkit/**/*.json\" dist/tools/devkit",
    "dev": "npm start -- -o",
    "pretest": "rimraf ./coverage ./testresults",
    "posttest": "node ./scripts/jest-join-coverage.js",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "postaffected:test": "node ./scripts/jest-join-coverage.js",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "add:component": "nx workspace-schematic angular-component",
    "postadd:component": "nx format:write",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "nx": "nx",
    "testjest": "jest",
    "release": "npx standard-version -an",
    "changelog:theme": "cd themes/angular-theme && npx standard-version",
    "changelog:components": "cd libs/angular-components && npx standard-version",
    "workspace-generator": "nx workspace-generator"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{apps,libs,tools,themes}/**/*.{ts,json,scss,html}": [
      "nx affected:lint --uncommitted --parallel --fix=true"
    ],
    "**/*.*": [
      "nx format:write --uncommited"
    ]
  },
  "dependencies": {
    "@angular/animations": "^11.2.0",
    "@angular/cdk": "^11.2.0",
    "@angular/cdk-experimental": "^11.2.0",
    "@angular/common": "^11.2.0",
    "@angular/compiler": "^11.2.0",
    "@angular/core": "^11.2.0",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.0",
    "@angular/material": "^11.2.0",
    "@angular/platform-browser": "^11.2.0",
    "@angular/platform-browser-dynamic": "^11.2.0",
    "@angular/router": "^11.2.0",
    "angular-plotly.js": "2.0.0",
    "core-js": "^2.5.4",
    "d3": "5.15.0",
    "elasticlunr": "github:weixsong/elasticlunr.js#f773890392760e0b3c68f0e65dbacb396dff297f",
    "lodash": "4.17.19",
    "ngx-color": "^6.1.0",
    "plotly.js": "1.54.1",
    "rxjs": "6.6.3",
    "smoothscroll-polyfill": "^0.4.4",
    "tslib": "2.0.3",
    "zone.js": "0.11.2",
    "@ngrx/component-store": "11.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.1",
    "@angular-devkit/core": "^11.2.0",
    "@angular/cli": "^11.2.0",
    "@angular/compiler-cli": "^11.2.0",
    "@angular/language-service": "^11.2.0",
    "@nrwl/angular": "11.3.1",
    "@nrwl/cli": "11.3.1",
    "@nrwl/cypress": "11.3.1",
    "@nrwl/jest": "11.3.1",
    "@nrwl/tao": "11.3.1",
    "@nrwl/workspace": "11.3.1",
    "@types/autoprefixer": "^9.7.2",
    "@types/d3": "5.7.2",
    "@types/fs-extra": "^8.0.0",
    "@types/jest": "26.0.15",
    "@types/lodash": "4.14.163",
    "@types/node": "12.12.38",
    "@types/node-sass": "^4.11.0",
    "@types/smoothscroll-polyfill": "0.3.1",
    "acorn": "^6.4.1",
    "autoprefixer": "^9.6.0",
    "canvas": "2.6.1",
    "codelyzer": "6.0.0",
    "commander": "^2.20.0",
    "cpx": "^1.5.0",
    "cypress": "^4.6.0",
    "cypress-image-snapshot": "^3.1.1",
    "dotenv": "6.2.0",
    "es-abstract": "1.14.2",
    "fs-extra": "^8.1.0",
    "git-config-path": "^2.0.0",
    "globby": "^11.0.0",
    "husky": "^4.2.5",
    "istanbul-api": "^2.1.6",
    "istanbul-lib-coverage": "^2.0.5",
    "jest": "26.6.1",
    "jest-canvas-mock": "2.3.0",
    "jest-junit": "12.0.0",
    "jest-preset-angular": "8.3.2",
    "js-yaml": "^3.13.1",
    "lint-staged": "^10.2.0",
    "ng-packagr": "11.2.0",
    "node-sass": "4.14.1",
    "node-sass-tilde-importer": "1.0.2",
    "parse-git-config": "^3.0.0",
    "postcss": "^7.0.17",
    "prettier": "2.2.1",
    "rimraf": "^2.7.1",
    "semver": "^6.3.0",
    "standard-version": "^9.0.0",
    "stylelint": "^13.3.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-formatter-pretty": "^1.1.2",
    "stylelint-scss": "^3.10.0",
    "ts-jest": "26.4.3",
    "ts-md5": "1.2.7",
    "ts-node": "9.1.1",
    "tsickle": "0.39.1",
    "tslint": "6.1.3",
    "typescript": "~4.0.3"
  }
}
