{
  "compilerOptions": {
    "target": "es2017",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": ["es2017", "dom", "dom.iterable"],
    "declaration": true,
    "sourceMap": true,
    "inlineSources": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "outDir": "./",
    "baseUrl": "./packages",
    "paths": {
      "@finastra/*": ["./*"]
    },
    "composite": true,
    "importHelpers": true
  },
  "file": [],
  "references": [{ "path": "logo" }, { "path": "divider" }]
}
